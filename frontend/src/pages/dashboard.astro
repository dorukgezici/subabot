---
import Layout from "@/layouts/Layout.astro";
import { DashboardPage } from "@/components";
import { getFeeds, getKeywords } from "@/lib/api";

const feeds = await getFeeds();
const keywords = await getKeywords();

const form = {
    feed: undefined,
    keyword: undefined,
    error: undefined,
};
---

<Layout title="Subabot">
    <DashboardPage
        client:only="svelte"
        data={{ feeds, keywords }}
        form={form}
    />
</Layout>
