---
import Layout from "@/layouts/Layout.astro";
import { DashboardPage } from "@/components";
import { getFeeds, getKeywords } from "@/lib/api";

const [feeds, keywords] = await Promise.all([
    getFeeds(fetch),
    getKeywords(fetch),
]);
const form = {
    feed: undefined,
    keyword: undefined,
    error: undefined,
};
---

<Layout title="Subabot">
    <DashboardPage data={{ feeds, keywords }} form={form} />
</Layout>
