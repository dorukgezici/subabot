---
import { Layout } from "@/layouts/Layout.astro";
import { Dashboard } from "@/components";
import { getFeeds, getKeywords } from "@/lib/api";

const [feeds, keywords] = await Promise.all([
    getFeeds(fetch),
    getKeywords(fetch),
]);
---

<Layout title="Subabot">
    <Dashboard data={(feeds, keywords)} />
</Layout>
